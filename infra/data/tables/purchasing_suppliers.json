{
    "table": "Purchasing.Suppliers",
    "datasource": "wideworldimporters",
    "description": "Contains information about suppliers who provide products to Wide World Importers.",
    "columns": [
        {
            "name": "SupplierID",
            "description": "Primary key. Unique identifier for each supplier."
        },
        {
            "name": "SupplierName",
            "description": "Full name of the supplier company."
        },
        {
            "name": "SupplierCategoryID",
            "description": "Foreign key to Purchasing.SupplierCategories."
        },
        {
            "name": "PrimaryContactPersonID",
            "description": "Foreign key to Application.People for main contact."
        },
        {
            "name": "AlternateContactPersonID",
            "description": "Foreign key to Application.People for alternate contact."
        },
        {
            "name": "DeliveryMethodID",
            "description": "Foreign key to Application.DeliveryMethods."
        },
        {
            "name": "DeliveryCityID",
            "description": "Foreign key to Application.Cities for delivery address."
        },
        {
            "name": "PostalCityID",
            "description": "Foreign key to Application.Cities for postal address."
        },
        {
            "name": "SupplierReference",
            "description": "Supplier's own reference code for Wide World Importers."
        },
        {
            "name": "BankAccountName",
            "description": "Name on the supplier's bank account."
        },
        {
            "name": "BankAccountBranch",
            "description": "Bank branch details."
        },
        {
            "name": "BankAccountCode",
            "description": "Bank routing code."
        },
        {
            "name": "BankAccountNumber",
            "description": "Supplier's bank account number."
        },
        {
            "name": "BankInternationalCode",
            "description": "International bank code (SWIFT/BIC)."
        },
        {
            "name": "PaymentDays",
            "description": "Standard payment terms in days."
        },
        {
            "name": "InternalComments",
            "description": "Internal notes about the supplier."
        },
        {
            "name": "PhoneNumber",
            "description": "Supplier's phone number."
        },
        {
            "name": "FaxNumber",
            "description": "Supplier's fax number."
        },
        {
            "name": "WebsiteURL",
            "description": "Supplier's website."
        },
        {
            "name": "DeliveryAddressLine1",
            "description": "First line of delivery address."
        },
        {
            "name": "DeliveryAddressLine2",
            "description": "Second line of delivery address."
        },
        {
            "name": "DeliveryPostalCode",
            "description": "Postal code for delivery address."
        },
        {
            "name": "PostalAddressLine1",
            "description": "First line of postal address."
        },
        {
            "name": "PostalAddressLine2",
            "description": "Second line of postal address."
        },
        {
            "name": "PostalPostalCode",
            "description": "Postal code for mailing address."
        }
    ]
}
